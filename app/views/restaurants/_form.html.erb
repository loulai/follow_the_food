<%= form_for @restaurant do |f| %>
	<%= f.label :name %><br>
	<%= f.text_field :name %><br>

	<%= f.label :city %><br>
	<%= f.text_field :city %><br>

	<%= f.label :description %><br>
	<%= f.text_area :description %><br>

	<%= f.label :location %><br>
	<%= f.text_area :location %><br>

	<%= f.submit %>

<% end %>